@model IEnumerable<KibrisSaatDunyasi.Core.Models.ProductCat>

@{
    ViewBag.Title = "ProductCats";
}

<h2>ProductCats</h2>

<p>
    @Html.ActionLink("Create New", "CreateProductCat")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.createdat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.catname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.createdat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.catname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
